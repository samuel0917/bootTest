<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wenchao.boottest.mapper.UserMapper">
    <select id="findAll" resultType="com.wenchao.boottest.entity.mybatis.User">
        select * from sys_login_user
    </select>

   <select id="findByName" resultType="com.wenchao.boottest.entity.mybatis.User">
        select * from sys_login_user where username=#{username}
    </select>

   
</mapper>